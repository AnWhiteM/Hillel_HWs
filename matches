import random

rnd_lst1 = []
rnd_lst2 = []
for i in range(random.randint(2, 10)):
    rnd_lst1.append((i + 1) * 3)
for i in range(random.randint(2, 10)):
    rnd_lst2.append((i + 1) * 5)
res = set(rnd_lst1).intersection(rnd_lst2)
print(rnd_lst1)
print(rnd_lst2)
if len(res) == 0:
    print("no matches")
else:
    print(res)
